{
  "name": "studs-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "flow && node scripts/start.js",
    "build": "flow && node scripts/build.js",
    "start:prod": "yarn build && yarn start",
    "flow": "flow",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "dependencies": {
    "@khanacademy/react-multi-select": "^0.3.3",
    "@sentry/browser": "^4.1.1",
    "babel-runtime": "6.26.0",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.5",
    "dotenv": "4.0.0",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.29.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.6",
    "react-feather": "^2.0.3",
    "react-ga": "^2.5.3",
    "react-google-charts": "^1.5.5",
    "react-hot-loader": "^3.1.3",
    "react-iframe": "^1.8.0",
    "react-inlinesvg": "^0.7.4",
    "react-intl": "2.4.0",
    "react-lazy-load": "^3.1.13",
    "react-markdown": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scroll": "^1.7.4",
    "react-slideshow-image": "^1.3.1",
    "react-textarea-autosize": "^5.2.1",
    "react-waypoint": "^7.3.4",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^1.1.3",
    "jest": "20.0.4",
    "lint-staged": "^8.0.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.15.1",
    "react-dev-utils": "^11.0.4",
    "style-loader": "0.19.0",
    "stylelint": "^8.4.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/app/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
